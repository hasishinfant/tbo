{
  "name": "travelsphere",
  "version": "1.0.0",
  "type": "module",
  "description": "AI-powered travel companion web application",
  "keywords": [
    "travel",
    "AI",
    "trip-planning",
    "react",
    "typescript"
  ],
  "author": "TravelSphere Team",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "clean": "rm -rf dist node_modules/.vite"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.20",
    "axios": "^1.13.4",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "fast-check": "^4.5.3",
    "gh-pages": "^6.3.0",
    "terser": "^5.46.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
